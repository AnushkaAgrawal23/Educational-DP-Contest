   "source": [
    "# Initialise the 2D dp array with -1 as value\n",
    "def solve(arr,i,x):\n",
    "    if x==0:\n",
    "        return 1\n",
    "    if i==0:\n",
    "        return 0\n",
    "    if(dp[i][x]!=-1):\n",
    "        return dp[i][x]\n",
    "    dp[i][x] = arr[i]*solve(arr,i-1,x-1) + (1-arr[i])*solve(arr,i-1,x)\n",
    "    return dp[i][x]"
   ]
